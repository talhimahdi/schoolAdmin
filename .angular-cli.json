{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ng5"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "assets/theme-assets/css/lib/chartist/chartist.min.css",
        "assets/theme-assets/css/lib/calendar2/pignose.calendar.min.css",
        "assets/theme-assets/css/lib/themify-icons.css",
        "assets/theme-assets/css/lib/owl.carousel.min.css",
        "assets/theme-assets/css/lib/owl.theme.default.min.css",
        "assets/theme-assets/css/lib/weather-icons.css",
        "assets/theme-assets/css/lib/menubar/sidebar.css",
        "assets/theme-assets/css/lib/unix.css",
        "assets/theme-assets/css/lib/bootstrap.min.css",
        "assets/theme-assets/css/style.css",
        "../node_modules/ng-pick-datetime/assets/style/picker.min.css",
        "../node_modules/primeng/resources/themes/omega/theme.css",
        "../node_modules/primeng/resources/primeng.min.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
